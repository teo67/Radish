val diamond = function(size) {
    whileval(val i = 1, i <= size, i = i + 1) {
        output("*" * i)r
    }
    whileval(val i = size - 1, i > 0, i = i - 1) {
        output(" " * (size - i) + "*" * i)r
    }
}r
diamond(20)r

